    "http",
    "https",
    "ftp",
    "file",
    "data",
    "protocol",
    "url"
  ],
  "author": "Nathan Rajlich <nathan@tootallnate.net> (http://n8.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TooTallNate/node-get-uri/issues"
  },
  "devDependencies": {
    "@types/debug": "4",
    "@types/fs-extra": "^8.0.1",
    "@types/ftp": "^0.3.30",
    "@types/ftpd": "^0.2.35",
    "@types/jest": "^29.5.1",
    "@types/node": "^14.18.42",
    "async-listen": "^2.1.0",
    "ftpd": "https://files-jg1s1zt9l.n8.io/ftpd-v0.2.14.tgz",
    "jest": "^29.5.0",
    "rimraf": "^3.0.0",
    "st": "^1.2.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@tootallnate/once": "2",
    "data-uri-to-buffer": "3",
    "debug": "4",
    "fs-extra": "^8.1.0",
    "ftp": "^0.3.10"
  },
  "engines": {
    "node": ">= 14"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "isolatedModules": true
      }
    },
    "verbose": false,
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ]
